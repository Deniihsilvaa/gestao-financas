{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "jsx": "react",  // Altere para 'react' para compatibilidade com JSX no TypeScript
    "lib": ["dom", "es2015"],
    "allowJs": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "moduleResolution": "node",
    "resolveJsonModule": true
  }
}
